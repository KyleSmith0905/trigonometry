{
  "name": "trionometry-transformer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "dev:android": "cap sync android && cap open android && npm run dev",
    "dev:stripe": "stripe listen --forward-to http://127.0.0.1:5001/trigonometry-simulator/us-central1/stripeWebhooks",
    "dev:functions": "cd functions && npm run serve:watch",
    "build": "run-p type-check build-only",
    "build:android": "npm run build && cap sync android && cap open android",
    "assets": "npx @capacitor/assets generate --iconBackgroundColor #f1f5f9 --iconBackgroundColorDark #0f172a --splashBackgroundColor #f1f5f9 --splashBackgroundColorDark #0f172a --android --ios",
    "assets:pwa": "npx @capacitor/assets generate --iconBackgroundColor #f1f5f9 --iconBackgroundColorDark #0f172a --splashBackgroundColor #f1f5f9 --splashBackgroundColorDark #0f172a --pwa --pwaManifestPath public/manifest.webmanifest",
    "assets:android": "npx @capacitor/assets generate --iconBackgroundColor #f1f5f9 --iconBackgroundColorDark #0f172a --splashBackgroundColor #f1f5f9 --splashBackgroundColorDark #0f172a --android",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@capacitor/android": "^4.7.3",
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^4.7.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/device": "^4.1.0",
    "@capacitor/preferences": "^4.0.2",
    "@stripe/stripe-js": "^1.52.1",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "firebase": "^9.20.0",
    "ionicons": "^7.1.0",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-meta": "^2.4.0",
    "vue-router": "^4.1.6",
    "vue-stripe-js": "^1.0.1",
    "vue3-spinners": "^1.2.2",
    "vuefire": "^3.1.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-pwa": "^0.14.7",
    "vue-tsc": "^1.2.0"
  }
}
